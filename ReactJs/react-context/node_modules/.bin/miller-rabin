#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/miller-rabin@4.0.1/node_modules/miller-rabin/bin/miller-rabin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/miller-rabin@4.0.1/node_modules/miller-rabin/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/miller-rabin@4.0.1/node_modules/miller-rabin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/miller-rabin@4.0.1/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/miller-rabin/bin/miller-rabin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/miller-rabin/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/miller-rabin/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../miller-rabin/bin/miller-rabin" "$@"
else 
  exec node  "$basedir/../miller-rabin/bin/miller-rabin" "$@"
fi
