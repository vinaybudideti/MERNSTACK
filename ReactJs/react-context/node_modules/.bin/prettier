#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/prettier@2.2.1/node_modules/prettier/bin-prettier.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/prettier@2.2.1/node_modules/prettier/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/prettier@2.2.1/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/prettier/bin-prettier.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/prettier/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin-prettier.js" "$@"
else 
  exec node  "$basedir/../prettier/bin-prettier.js" "$@"
fi
