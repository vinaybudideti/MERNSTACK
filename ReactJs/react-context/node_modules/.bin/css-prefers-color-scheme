#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/css-prefers-color-scheme@3.1.1/node_modules/css-prefers-color-scheme/cli.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/css-prefers-color-scheme@3.1.1/node_modules/css-prefers-color-scheme/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/css-prefers-color-scheme@3.1.1/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/css-prefers-color-scheme/cli.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/css-prefers-color-scheme/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../css-prefers-color-scheme/cli.js" "$@"
else 
  exec node  "$basedir/../css-prefers-color-scheme/cli.js" "$@"
fi
