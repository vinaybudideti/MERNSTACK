#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/husky@4.3.8/node_modules/husky/bin/run.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/husky@4.3.8/node_modules/husky/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/husky@4.3.8/node_modules/husky/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/husky@4.3.8/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/husky/bin/run.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/husky/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/husky/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky/bin/run.js" "$@"
else 
  exec node  "$basedir/../husky/bin/run.js" "$@"
fi
