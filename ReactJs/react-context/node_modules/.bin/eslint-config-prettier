#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/eslint-config-prettier@8.1.0/node_modules/eslint-config-prettier/bin/cli.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/eslint-config-prettier@8.1.0/node_modules/eslint-config-prettier/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/eslint-config-prettier@8.1.0/node_modules/eslint-config-prettier/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/eslint-config-prettier@8.1.0/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/eslint-config-prettier/bin/cli.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/eslint-config-prettier/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/eslint-config-prettier/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
else 
  exec node  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
fi
