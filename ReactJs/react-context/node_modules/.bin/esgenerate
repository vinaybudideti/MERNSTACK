#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/bin/esgenerate.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/escodegen@2.0.0/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/escodegen/bin/esgenerate.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/escodegen/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/escodegen/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../escodegen/bin/esgenerate.js" "$@"
else 
  exec node  "$basedir/../escodegen/bin/esgenerate.js" "$@"
fi
