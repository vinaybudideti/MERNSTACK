#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/regjsparser@0.6.9/node_modules/regjsparser/bin/parser/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/regjsparser@0.6.9/node_modules/regjsparser/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/regjsparser@0.6.9/node_modules/regjsparser/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/regjsparser@0.6.9/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/regjsparser/bin/parser/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/regjsparser/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/regjsparser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../regjsparser/bin/parser" "$@"
else 
  exec node  "$basedir/../regjsparser/bin/parser" "$@"
fi
