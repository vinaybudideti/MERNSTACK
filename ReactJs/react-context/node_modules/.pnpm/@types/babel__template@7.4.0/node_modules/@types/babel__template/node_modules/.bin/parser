#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@babel/parser@7.14.7/node_modules/@babel/parser/bin/babel-parser.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@babel/parser@7.14.7/node_modules/@babel/parser/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@babel/parser@7.14.7/node_modules/@babel/parser/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@babel/parser@7.14.7/node_modules/@babel/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@babel/parser@7.14.7/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@babel/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@types/babel__template@7.4.0/node_modules/@types/babel__template/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@types/babel__template@7.4.0/node_modules/@types/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@types/babel__template@7.4.0/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/@types/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../@babel/parser@7.14.7/node_modules/@babel/parser/bin/babel-parser.js" "$@"
else 
  exec node  "$basedir/../../../../../../../@babel/parser@7.14.7/node_modules/@babel/parser/bin/babel-parser.js" "$@"
fi
