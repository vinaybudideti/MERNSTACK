#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/jest@26.6.0/node_modules/jest/bin/jest.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/jest@26.6.0/node_modules/jest/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/jest@26.6.0/node_modules/jest/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/jest@26.6.0/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/jest-watch-typeahead@0.6.1_jest@26.6.0/node_modules/jest-watch-typeahead/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/jest-watch-typeahead@0.6.1_jest@26.6.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest@26.6.0/node_modules/jest/bin/jest.js" "$@"
else 
  exec node  "$basedir/../../../../../jest@26.6.0/node_modules/jest/bin/jest.js" "$@"
fi
