#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/cssesc/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/cssesc@3.0.0/node_modules/cssesc/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/cssesc@3.0.0/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/postcss-selector-parser@6.0.6/node_modules/postcss-selector-parser/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/postcss-selector-parser@6.0.6/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
else 
  exec node  "$basedir/../../../../../cssesc@3.0.0/node_modules/cssesc/bin/cssesc" "$@"
fi
