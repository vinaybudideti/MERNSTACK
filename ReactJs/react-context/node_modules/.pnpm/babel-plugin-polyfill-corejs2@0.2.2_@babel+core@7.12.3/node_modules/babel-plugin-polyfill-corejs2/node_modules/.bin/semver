#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/semver.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/semver@6.3.0/node_modules/semver/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/semver@6.3.0/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/babel-plugin-polyfill-corejs2@0.2.2_@babel+core@7.12.3/node_modules/babel-plugin-polyfill-corejs2/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/babel-plugin-polyfill-corejs2@0.2.2_@babel+core@7.12.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@6.3.0/node_modules/semver/bin/semver.js" "$@"
else 
  exec node  "$basedir/../../../../../semver@6.3.0/node_modules/semver/bin/semver.js" "$@"
fi
