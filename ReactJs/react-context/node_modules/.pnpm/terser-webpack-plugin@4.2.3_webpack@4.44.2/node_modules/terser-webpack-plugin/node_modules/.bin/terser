#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/terser@5.7.1/node_modules/terser/bin/terser/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/terser@5.7.1/node_modules/terser/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/terser@5.7.1/node_modules/terser/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/terser@5.7.1/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/terser-webpack-plugin@4.2.3_webpack@4.44.2/node_modules/terser-webpack-plugin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/terser-webpack-plugin@4.2.3_webpack@4.44.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@5.7.1/node_modules/terser/bin/terser" "$@"
else 
  exec node  "$basedir/../../../../../terser@5.7.1/node_modules/terser/bin/terser" "$@"
fi
