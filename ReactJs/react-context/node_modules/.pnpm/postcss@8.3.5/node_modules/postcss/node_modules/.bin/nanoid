#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/nanoid@3.1.23/node_modules/nanoid/bin/nanoid.cjs/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/nanoid@3.1.23/node_modules/nanoid/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/nanoid@3.1.23/node_modules/nanoid/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/nanoid@3.1.23/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/postcss@8.3.5/node_modules/postcss/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/postcss@8.3.5/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nanoid@3.1.23/node_modules/nanoid/bin/nanoid.cjs" "$@"
else 
  exec node  "$basedir/../../../../../nanoid@3.1.23/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
