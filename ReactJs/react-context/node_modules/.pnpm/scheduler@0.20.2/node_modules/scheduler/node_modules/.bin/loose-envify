#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/cli.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/scheduler@0.20.2/node_modules/scheduler/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/scheduler@0.20.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
fi
