#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/browserslist@4.14.2/node_modules/browserslist/cli.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/browserslist@4.14.2/node_modules/browserslist/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/browserslist@4.14.2/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/react-dev-utils@11.0.4/node_modules/react-dev-utils/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/react-dev-utils@11.0.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.14.2/node_modules/browserslist/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../browserslist@4.14.2/node_modules/browserslist/cli.js" "$@"
fi
