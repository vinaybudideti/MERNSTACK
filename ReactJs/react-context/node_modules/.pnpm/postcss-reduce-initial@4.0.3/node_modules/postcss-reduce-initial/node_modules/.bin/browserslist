#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/browserslist@4.16.6/node_modules/browserslist/cli.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/browserslist@4.16.6/node_modules/browserslist/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/browserslist@4.16.6/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/postcss-reduce-initial@4.0.3/node_modules/postcss-reduce-initial/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/postcss-reduce-initial@4.0.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.16.6/node_modules/browserslist/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../browserslist@4.16.6/node_modules/browserslist/cli.js" "$@"
fi
