#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/fixtures/cli.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/fixtures/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/import-local@2.0.0/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/webpack-dev-server@3.11.1_webpack@4.44.2/node_modules/webpack-dev-server/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/webpack-dev-server@3.11.1_webpack@4.44.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@2.0.0/node_modules/import-local/fixtures/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../import-local@2.0.0/node_modules/import-local/fixtures/cli.js" "$@"
fi
