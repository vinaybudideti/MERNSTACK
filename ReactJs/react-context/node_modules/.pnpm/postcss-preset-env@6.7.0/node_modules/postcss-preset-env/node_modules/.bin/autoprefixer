#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/autoprefixer@9.8.6/node_modules/autoprefixer/bin/autoprefixer/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/autoprefixer@9.8.6/node_modules/autoprefixer/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/autoprefixer@9.8.6/node_modules/autoprefixer/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/autoprefixer@9.8.6/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/postcss-preset-env@6.7.0/node_modules/postcss-preset-env/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/postcss-preset-env@6.7.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../autoprefixer@9.8.6/node_modules/autoprefixer/bin/autoprefixer" "$@"
else 
  exec node  "$basedir/../../../../../autoprefixer@9.8.6/node_modules/autoprefixer/bin/autoprefixer" "$@"
fi
