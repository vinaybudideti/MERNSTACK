#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/which@2.0.2/node_modules/which/bin/node-which/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/which@2.0.2/node_modules/which/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/which@2.0.2/node_modules/which/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/which@2.0.2/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node-notifier@8.0.2/node_modules/node-notifier/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node-notifier@8.0.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../which@2.0.2/node_modules/which/bin/node-which" "$@"
else 
  exec node  "$basedir/../../../../../which@2.0.2/node_modules/which/bin/node-which" "$@"
fi
