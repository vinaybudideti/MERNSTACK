#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/he/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/he@1.2.0/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
else 
  exec node  "$basedir/../../../../../he@1.2.0/node_modules/he/bin/he" "$@"
fi
