#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/mime@2.5.2/node_modules/mime/cli.js/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/mime@2.5.2/node_modules/mime/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/mime@2.5.2/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules:/home/nxtwave/reactjs/sessions/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/webpack-dev-middleware@3.7.3_webpack@4.44.2/node_modules/webpack-dev-middleware/node_modules:/home/nxtwave/reactjs/sessions/react-context/node_modules/.pnpm/webpack-dev-middleware@3.7.3_webpack@4.44.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@2.5.2/node_modules/mime/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../mime@2.5.2/node_modules/mime/cli.js" "$@"
fi
